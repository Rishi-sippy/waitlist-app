datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model WaitlistEntry {
  id        String   @id @default(uuid())
  shop      String
  email     String?
  phone     String?
  productId String
  createdAt DateTime @default(now())
}

model Shop {
  id        String   @id
  domain    String   @unique
  accessToken String
  createdAt DateTime @default(now())
}
